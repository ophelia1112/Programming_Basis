   "source": [
    "# 转换日期\n",
    "# 从1取去掉其他元素，格式化的格式为年月日时分秒，查pandas官方文档\n",
    "df_new['stime']=pd.to_datetime(df_new['stime'].str[1:],format='%d/%b/%Y:%H:%M:%S') # \n",
    "df_new"
   ]
  },
