    "import pandas as pd\n",
    "from pyecharts import options as opts\n",
    "from pyecharts.charts import Line"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 658,
   "id": "f30cdd37-69e0-4676-a7e2-08392412bf84",
