   "source": [
    "# 便捷方法，用df全部替换\n",
    "needcode_cat_columns=['Pclass','Sex','SibSp','Parch','Embarked']\n",
    "df_code=pd.get_dummies(\n",
    "    df_train,\n",
    "    # 要转码的列\n",
    "    columns=needcode_cat_columns,\n",
    "    # 即使是空值也要做转码\n",
    "    dummy_na=True,\n",
    "    # 移除列中的一列,谨防dummy varible trap\n",
    "    drop_first=True\n",
    ") # 输出布尔值\n",
    "# 转换布尔值\n",
    "df_code = df_code.astype(int)\n",
    "df_code"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "764343d3-3d1c-4dc3-a5eb-17ef524c92a0",
   "metadata": {},
   "outputs": [],
   "source": []
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "41351a23-a6bc-46c1-a20e-9bdfaa0ebafb",
   "metadata": {},
   "outputs": [],
   "source": [
    "# 机器学习训练"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 733,
   "id": "b36ea900-8f4e-4ff0-967d-c981112fd911",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "0      0\n",
       "1      1\n",
       "2      1\n",
       "3      1\n",
       "4      0\n",
       "      ..\n",
       "886    0\n",
       "887    1\n",
       "888    0\n",
       "889    1\n",
