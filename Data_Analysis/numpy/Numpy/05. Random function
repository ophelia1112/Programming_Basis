   "source": [
    "# 加入噪声（完美数据不存在，会有很多噪声）\n",
    "x=np.linspace(-10,10,100) # 最小数，最大数，中间生成100点\n",
    "y=np.sin(x)+np.random.rand(len(x)) # 根据x的长度生成每一个对应的噪声\n",
    "plt.plot(x,y) # 绘图\n",
    "plt.show()"
   ]
